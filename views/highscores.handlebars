{{> nav-bar }}

<div class="container">
  <div class="divider"></div>
  <div class="row">
    <div class="col s8 offset-s2 center-align">
      <table class="birds-table striped">
        <thead class="table-head">
          <tr>
            <th>Rank</th>
            <th>User Name</th>
            <th>Score</th>

          </tr>
        </thead>

        <tbody class="table-body">
          {{#each users}}
          <tr>
            <td>{{@index}}</td>
            <td><a href="/users/{{id}}" class="">{{user_name}}</td></a>
            {{!-- <td><button class="btn" data-id="{{this.id}}" />{{id}}. {{common_name}}</td> --}}
            <td>{{score}}</td>
          </tr>

          {{/each}}


        </tbody>
      </table>
    </div>
  </div>
</div>

