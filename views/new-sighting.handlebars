{{> nav-bar }}



<button id="upload_widget" class="cloudinary-button">Upload files</button>
<p class="hide" id="successMessage">Upload was sucessful</p>
<p class="hide" id="failMessage">Upload failed</p>


<div class="container">
  <div class="row">
    <form class="col s6">
      <div class="input-field row">
        <select>
          <option value="" disabled selected>Choose your username</option>
          {{#each users}}
          <option value={{dataValues.id}}>{{dataValues.user_name}}</option>
          {{/each}}
        </select>
      </div>
      <div class="input-field row">
        <select>
          <option value="" disabled selected>Select a bird</option>
          {{#each birds}}
          <option value={{dataValues.id}}>{{dataValues.common_name}}</option>
          {{/each}}
        </select>
      </div>
      <div class="row">
        <div class="input-field col-12">
          <i class="material-icons prefix">mode_edit</i>
          <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
          <label for="icon_prefix2">Comments</label>
        </div>
      </div>
    </form>

  </div>
</div>
</div>

<script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript"></script>
<script src="../assets/js/new-sighting.js"></script>