<tr>
  <td><a href="/sightings/{{id}}" class="h-color-black">{{createdAt}}</a></td>
  <td class="align-wrapper"><a href="/birds/{{Bird.id}}" class="h-color-black">{{Bird.common_name}}</a></td>
  <td class="center-align"><a href="/sightings/{{id}}" class="h-color-black"><img
        class="bird-img" src="{{image_URL}}" /></a></td>
  
  <td>{{location}}</td>
  <td class="center-align">{{Bird.points}}</td>
  <td class="center-align"><a href="/sightings/{{id}}/edit"><i class="fas fa-edit"></i></a></td>
  <td class="center-align delete-sighting" data-id="{{id}}"><a href=""><i class="fas fa-trash-alt"></i></a></td>
</tr>